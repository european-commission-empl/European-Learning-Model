    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Organisation</rdfs:label>
    <rdfs:comment xml:lang="en">A legal person / registered organisation.</rdfs:comment>
