    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Awarding Body</rdfs:label>
    <rdfs:comment xml:lang="en">An awarding body.</rdfs:comment>
    <dc:identifier>elm:AwardingBody</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Amount">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Amount</rdfs:label>
    <rdfs:comment xml:lang="en">An amount.</rdfs:comment>
    <dc:identifier>elm:Amount</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/AwardingOpportunity">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Awarding Opportunity</rdfs:label>
    <rdfs:comment xml:lang="en">An awarding opportunity describes how, where, from whom and when a claim may be obtained.</rdfs:comment>
    <dc:identifier>elm:AwardingOpportunity</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/DisplayParameter">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Display Parameter</rdfs:label>
    <rdfs:comment xml:lang="en">The display parameters.</rdfs:comment>
    <dc:identifier>elm:DisplayParameter </dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/LearningAchievementSpecification">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Specification"/>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Learning Achievement Specification</rdfs:label>
    <rdfs:comment xml:lang="en">A description of what a person may learn using the opportunity, expressed as learning outcomes. A specification of learning achievement.</rdfs:comment>
    <dc:identifier>elm:LearningAchievementSpecification</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Group">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Group"/>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Group</rdfs:label>
    <rdfs:comment xml:lang="en">An entity that represents collection of Agents.</rdfs:comment>
    <dc:identifier>elm:Group</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Grant">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Grant</rdfs:label>
    <rdfs:comment xml:lang="en">A Grant.</rdfs:comment>
    <dc:identifier>elm:Grant</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/LearningOutcome">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Learning Outcome</rdfs:label>
    <rdfs:comment xml:lang="en">A statement regarding what a learner knows, understands and is able to do on completion of a learning process, which are defined in terms of knowledge, skills and responsibility and autonomy.</rdfs:comment>
    <dc:identifier>elm:LearningOutcome</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Claim">
    <owl:disjointWith>
      <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Specification"/>
    </owl:disjointWith>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Claim</rdfs:label>
    <rdfs:comment xml:lang="en">A claim made by an issuer.</rdfs:comment>
    <dc:identifier>elm:Claim</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Proof">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Proof</rdfs:label>
    <rdfs:comment xml:lang="en">The cryptographic proof that can be used to detect tampering and verify the authorship of a credential or presentation.</rdfs:comment>
    <dc:identifier>elm:Proof</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/ShortenedGrading">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Shortened Grading</rdfs:label>
    <rdfs:comment xml:lang="en">Indicator of how well the student was graded when compared to other students.</rdfs:comment>
    <dc:identifier>elm:ShortenedGrading</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Address">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Address</rdfs:label>
    <rdfs:comment xml:lang="en">An Address.</rdfs:comment>
    <dc:identifier>elm:Address</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/Evidence">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Evidence</rdfs:label>
    <rdfs:comment xml:lang="en">The evidence.</rdfs:comment>
    <dc:identifier>elm:Evidence</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/PriceDetail">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Price Detail</rdfs:label>
    <rdfs:comment xml:lang="en">The price details. The details about a price or price category.</rdfs:comment>
    <dc:identifier>elm:PriceDetail</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/AwardingProcess">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Awarding Process</rdfs:label>
    <rdfs:comment xml:lang="en">The process of an organisation making a Claim to person based on a Specification. It is used to specify the organisation that awarded the Claim to the individual, or the country or region where the Claim was made, and optionally the date.</rdfs:comment>
    <dc:identifier>elm:AwardingProcess</dc:identifier>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://data.europa.eu/snb/model/elm/CredentialStatus">
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/snb/model/elm"/>
    <rdfs:label xml:lang="en">Credential Status</rdfs:label>
    <rdfs:comment xml:lang="en">The credential status. It defines discovery information about the current status of a Verifiable Credential.</rdfs:comment>
