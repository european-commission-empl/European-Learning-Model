#------------------------------------------------------------------
#------      Accreditation class and properties shapes       ------
#------------------------------------------------------------------
edc:AccreditationShape 
  a sh:NodeShape; 
  sh:targetClass elm:Accreditation; 
  rdfs:comment "The quality assurance or licensing of an organisation or a qualification. An accreditation instance can be used to specify information about: (1) the quality assurance and/or licensing of an organisation, (2) the quality assurance and/or licensing of an organisation with respect to a specific qualification."@en;
  rdfs:label "Accreditation"@en; 
  sh:property edc:Accreditation-adms_identifier;
  sh:property edc:Accreditation-dc_type;
  sh:property edc:Accreditation-dc_title;
  sh:property edc:Accreditation-dc_description;
  sh:property edc:Accreditation-foaf_homepage;
  sh:property edc:Accreditation-dc_issued;
  sh:property edc:Accreditation-elm_additionalNote;
  sh:property edc:Accreditation-elm_supplementaryDocument;
  sh:property edc:Accreditation-elm_decision;
  sh:property edc:Accreditation-elm_report;
  sh:property edc:Accreditation-elm_organisation;
  sh:property edc:Accreditation-elm_limitQualification;
  sh:property edc:Accreditation-elm_limitField;
  sh:property edc:Accreditation-elm_limitEQFLevel;
  sh:property edc:Accreditation-elm_limitJurisdiction;
  sh:property edc:Accreditation-elm_limitCredentialType;
  sh:property edc:Accreditation-elm_accreditingAgent;
  sh:property edc:Accreditation-elm_reviewDate;
  sh:property edc:Accreditation-elm_expiryDate;
  sh:property edc:Accreditation-elm_landingPage;
  sh:property edc:Accreditation-elm_status; 
  sh:property edc:Accreditation-dc_modified;
  sh:property edc:Accreditation-dc_valid;
  .
  
edc:Accreditation-dc_title  
  sh:path dc:title;
  sh:datatype rdf:langString;
  sh:minCount 1;
  sh:description "The title of the accreditation. One value per language is permitted. "@en;
  sh:name "title"@en;
  sh:uniqueLang true ;
  sh:severity sh:Violation;
  #sh:order 0;
  .

edc:Accreditation-dc_description
  a sh:PropertyShape; 
  sh:path dc:description;
  sh:datatype rdf:langString;
  sh:description "A free text description of the accreditation."@en;
  sh:name "description"@en;
  sh:uniqueLang true ;
  sh:severity sh:Violation;
  #sh:order 1;
  .

edc:Accreditation-adms_identifier
  a sh:PropertyShape; 
  sh:path adms:identifier;
  sh:class elm:Identifier;
  sh:description "An identifier of the accreditation."@en;
  sh:name "identifier"@en;
  sh:severity sh:Violation;
  #sh:order 2; 
  .
 edc:Accreditation-elm_additionalNote
  a sh:PropertyShape; 
  sh:path elm:additionalNote;
  sh:maxCount 1;
  sh:class elm:Note;
  sh:description "An additional free text note about the accreditation."@en;
  sh:name "more information"@en;
  sh:severity sh:Violation;
  #sh:order 3; 
  . 
edc:Accreditation-elm_supplementaryDocument
  a sh:PropertyShape; 
  sh:path elm:supplementaryDocument;
  sh:class elm:WebResource;
  sh:description "A public web document containing additional documentation about the accreditation."@en;
  sh:name "other documents"@en;
  sh:severity sh:Violation;
  #sh:order 4;
  .
edc:Accreditation-dc_type
  a sh:PropertyShape; 
  sh:path dc:type;
  sh:class skos:Concept;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The type of the accreditation. It should be provided using the EDC Controlled List of Accreditation Types, (http://publications.europa.eu/resource/dataset/accreditation)."@en;
  sh:name "type"@en;
  sh:severity sh:Violation;
  #sh:order 5; 
  . 
 edc:Accreditation-foaf_homepage
  a sh:PropertyShape; 
  sh:path foaf:homepage;
  sh:class elm:WebResource;
  sh:description "The homepage of the accreditation."@en;
  sh:name "homepage"@en;
  sh:severity sh:Violation;
  #sh:order 6;
  . 

edc:Accreditation-dc_issued
  a sh:PropertyShape; 
  sh:path dc:issued;
  sh:datatype xsd:dateTime;
  sh:maxCount 1;
  sh:description "The date when the accreditation was formally approved/issued."@en;
  sh:name "accreditation date issued"@en;
  sh:severity sh:Violation;
  #sh:order 7;
  . 
edc:Accreditation-elm_decision
  a sh:PropertyShape; 
  sh:path elm:decision;
  sh:class skos:Concept;
  sh:maxCount 1;
  sh:description "The quality decision issued by the quality assuring authority. It shoul dbe provided using the DEQAR controlled list."@en;
  sh:name "decision"@en;
  sh:severity sh:Violation;
  #sh:order 8;
  .

edc:Accreditation-elm_report
  a sh:PropertyShape; 
  sh:path elm:report;
  sh:class elm:WebResource;
  sh:maxCount 1;
  sh:description "A publicly accessible report of the quality assurance decision."@en;
  sh:name "report"@en;
  sh:severity sh:Violation;
  #sh:order 9; 
  .

edc:Accreditation-elm_organisation
  a sh:PropertyShape; 
  sh:path elm:organisation;
  sh:class elm:Organisation;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The organisation whose activities are being accredited."@en;
  sh:name "organisation"@en;
  sh:severity sh:Violation;
  #sh:order 10;
  .

edc:Accreditation-elm_limitQualification
  a sh:PropertyShape; 
  sh:path elm:limitQualification;
  sh:class elm:Qualification;
  sh:maxCount 1;
  sh:description "The qualification that was accredited."@en;
  sh:name "qualification accredited"@en;
  sh:severity sh:Violation;
  #sh:order 11;
  .

edc:Accreditation-elm_limitField
  a sh:PropertyShape; 
  sh:path elm:limitField;
  sh:class skos:Concept;
  sh:description "The field of education for which the accreditation is valid. It should be provided using the ISCED-F controlled vocabulary."@en;
  sh:name "accredited for thematic area"@en;
  sh:severity sh:Violation;
  #sh:order 12;
  .
  
edc:Accreditation-elm_limitEQFLevel
  a sh:PropertyShape; 
  sh:path elm:limitEQFLevel;
  sh:class skos:Concept;
  sh:description "The European Qualification Framework level for which the accreditation is valid. It should be provided using the EQF controlled vocabulary."@en;
  sh:name "accredited for EQF level"@en;
  sh:severity sh:Violation;
  #sh:order 13;
  .

edc:Accreditation-elm_limitJurisdiction
  a sh:PropertyShape; 
  sh:path elm:limitJurisdiction;
  sh:class skos:Concept;
  sh:description "The jurisdiction for which the accreditation is valid. It should be provided using the Administrative territorial unit Authority Table (ATU) controlled vocabulary."@en;
  sh:name "accredited in jurisdiction"@en;
  sh:severity sh:Violation;
  #sh:order 14;
  .
edc:Accreditation-elm_limitCredentialType
  a sh:PropertyShape; 
  sh:path elm:limitCredentialType;
  sh:class skos:Concept;
  sh:description "The credential type for which the accreditation is valid. It MUST be provided using the Credential Type controlled vocabulary."@en;
  sh:name "limit credential type"@en;
  sh:severity sh:Violation;
  #sh:order 15;
  .
edc:Accreditation-elm_accreditingAgent
  a sh:PropertyShape; 
  sh:path elm:accreditingAgent;
  sh:class elm:Organisation;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The Quality Assuring Authority (i.e., assurer)."@en;
  sh:name "accrediting agent"@en;
  sh:severity sh:Violation;
  #sh:order 16;
  .

edc:Accreditation-elm_reviewDate
  a sh:PropertyShape; 
  sh:path elm:reviewDate;
  sh:datatype xsd:dateTime;
  sh:maxCount 1;
  sh:description "The date when the accreditation has to be reviewed."@en;
  sh:name "review date"@en;
  sh:severity sh:Violation;
  #sh:order 17;
  .

edc:Accreditation-elm_expiryDate
  a sh:PropertyShape; 
  sh:path elm:expirydate;
  sh:datatype xsd:dateTime;
  sh:maxCount 1;
  sh:description "The date when the accreditation expires or was expired."@en;
  sh:name "expiry date"@en;
   sh:severity sh:Violation;
   #sh:order 18;
  .

edc:Accreditation-elm_landingPage
  a sh:PropertyShape; 
  sh:path elm:landingPage;
  sh:class elm:WebResource;
  sh:description "The landing page of the accreditation."@en;
  sh:name "landing page"@en;
  sh:severity sh:Violation;
  #sh:order 19;
  .
edc:Accreditation-elm_status 
    a sh:PropertyShape; 
    sh:path elm:status;
    sh:class skos:Concept;
    sh:maxCount 1;
    sh:description "The publication status of the accreditation."@en;
    sh:name "status"@en;
    sh:severity sh:Violation;
    #sh:order 20;
  . 
edc:Accreditation-dc_modified
    a sh:PropertyShape; 
    sh:path dc:modified;
    sh:datatype xsd:dateTime;
    sh:maxCount 1;
    sh:description "The date when the accreditation was last updated since it was published."@en;
    sh:name "last modification date"@en;
    sh:severity sh:Violation;
    #sh:order 21;
  .
  edc:Accreditation-dc_valid
    a sh:PropertyShape; 
    sh:path dc:valid;
    sh:datatype xsd:dateTime;
    sh:maxCount 1;
    sh:description "The date when the accreditation became valid."@en;
    sh:name "validation  date"@en;
    sh:severity sh:Violation;
    #sh:order 22;
  .
