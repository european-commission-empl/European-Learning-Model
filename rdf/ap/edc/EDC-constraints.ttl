#------ Amount class and properties shapes --------
edc:AmountShape 
  a sh:NodeShape; 
  sh:targetClass elm:Amount; 
  rdfs:comment "An amount."@en; 
  rdfs:label "Amount "@en; 
  sh:property edc:Amount-elm_unit;
  sh:property edc:Amount-elm_value;
  .

edc:Amount-elm_unit
  a sh:PropertyShape; 
  sh:path elm:unit;
  sh:class skos:Concept;
  sh:path elm:unit;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "A code indicating the currency in which the amount is indicated/expressed. It should be provided using the Currencies Named Authority List."@en;
  sh:name "unit"@en;
  sh:severity sh:Violation;
  #sh:order 0;
.

edc:Amount-elm_value
  a sh:PropertyShape; 
  sh:path elm:value;
  sh:datatype xsd:decimal;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The numeric value (i.e., price, salary, etc.)."@en;
  sh:name "amount content"@en;
  sh:severity sh:Violation;
  #sh:order 1;
.

#------ Awarding Opportunity class and properties shapes --------

edc:AwardingOpportunityShape 
  a sh:NodeShape; 
  sh:targetClass elm:AwardingOpportunity; 
  rdfs:comment "An awarding opportunity describes how, where, from whom and when a claim may be obtained."@en; 
  rdfs:label "Awarding Opportunity"@en; 
  sh:property edc:AwardingOpportunity-adms_identifier;
  sh:property edc:AwardingOpportunity-elm_location;
  sh:property edc:AwardingOpportunity-dc_temporal;
  sh:property edc:AwardingOpportunity-elm_awardingBody;
  sh:property edc:AwardingOpportunity-elm_learningAchievementSpecification;
  .
edc:AwardingOpportunity-adms_identifier
  a sh:PropertyShape; 
  sh:path adms:identifier;
  sh:class elm:Identifier;
  sh:description "An identifier of the awarding opportunity."@en;
  sh:name "identifier"@en;
  sh:severity sh:Violation;
  #sh:order 0;
  .
 edc:AwardingOpportunity-elm_location
  a sh:PropertyShape;  
  sh:path elm:location;
  sh:class dc:Location;
  sh:maxCount 1;
  sh:description "The location where the awarding activities take/took place (country/region where the qualification is awarded)."@en;
  sh:name "location"@en;
  sh:severity sh:Violation;
  #sh:order 1;
. 
edc:AwardingOpportunity-dc_temporal
  a sh:PropertyShape; 
  sh:path dc:temporal;
  sh:class dc:PeriodOfTime;
  sh:maxCount 1;
  sh:description "The temporal duration, associated with a period of time when the awarding activities take place. If not specified it is undefined (“not known”)."@en;
  sh:name "temporal "@en;
  sh:severity sh:Violation;
  #sh:order 2;
  . 
edc:AwardingOpportunity-elm_awardingBody
  a sh:PropertyShape;  
  sh:path elm:awardingBody;
  sh:class elm:Agent;
  sh:minCount 1;
  sh:description "The awarding body related to this awarding activity (i.e., the organisation that issues the qualification). Only in cases of co-awarding/co-graduation, where a qualification is issued to an individual by two or more organisations, the cardinality is greater than 1."@en;
  sh:name "awarding body"@en;
  sh:severity sh:Violation;
  #sh:order 3;
.
edc:AwardingOpportunity-elm_learningAchievementSpecification
  a sh:PropertyShape;  
  sh:path elm:learningAchievementSpecification;
  sh:class elm:LearningAchievementSpecification;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The awarded learning achievement specification."@en;
  sh:name "awarded learning achievement specification"@en;
  sh:severity sh:Violation;
  #sh:order 4;
.

#------ Awarding Process class and properties shapes --------
edc:AwardingProcessShape 
  a sh:NodeShape; 
  sh:targetClass elm:AwardingProcess; 
  rdfs:comment "The process of an organisation making a Claim to person based on a Specification. It is used to specify the organisation that awarded the Claim to the individual, or the country or region where the Claim was made, and optionally the date."@en; 
  rdfs:label "Awarding Process "@en; 
  sh:property edc:AwardingProcess-adms_identifier;
  sh:property edc:AwardingProcess-dc_description;
  sh:property edc:AwardingProcess-elm_location;
  sh:property edc:AwardingProcess-elm_additionalNote;
  sh:property edc:AwardingProcess-elm_used;
  sh:property edc:AwardingProcess-elm_awards;
  sh:property edc:AwardingProcess-elm_awardingBody;
  sh:property edc:AwardingProcess-elm_awardingDate;
  sh:property edc:AwardingProcess-elm_educationalSystemNote;
.
edc:AwardingProcess-dc_description
  a sh:PropertyShape; 
  sh:path dc:description;
  sh:datatype rdf:langString;
  sh:description "A free text description of the awarding process."@en;
  sh:name "description"@en;
  sh:uniqueLang true ;
  sh:severity sh:Violation;
  #sh:order 0;
  .
edc:AwardingProcess-adms_identifier
  a sh:PropertyShape; 
  sh:path adms:identifier;
  sh:class elm:Identifier;
  sh:description "An identifier of the awarding process."@en;
  sh:name "identifier"@en;
  sh:severity sh:Violation;
  #sh:order 1;
  .
edc:AwardingProcess-elm_location
  a sh:PropertyShape;  
  sh:path elm:location;
  sh:class dc:Location;
  sh:maxCount 1;
  sh:description "The location where the awarding activity took place (country/region where the qualification was awarded)."@en;
  sh:name "location"@en;
  sh:severity sh:Violation;
  #sh:order 2;
.
edc:AwardingProcess-elm_additionalNote
  a sh:PropertyShape; 
  sh:path elm:additionalNote;
  sh:class elm:Note;
  sh:maxCount 1;
  sh:description "An additional free text note about the awarding process."@en;
  sh:name "more information"@en;
  sh:severity sh:Violation;
  #sh:order 3;
  .  
edc:AwardingProcess-elm_used
  a sh:PropertyShape;  
  sh:path elm:used;
  sh:class elm:LearningAssessment;
  sh:description "The assessment that provided the basis for this awarding."@en;
  sh:name "assessment utilised"@en;
  sh:severity sh:Violation;
  #sh:order 4;
.

edc:AwardingProcess-elm_awards
  a sh:PropertyShape;  
  sh:path elm:awards;
  #sh:class elm:Claim;
  sh:node edc:ClaimNodeShape;
  sh:minCount 1;
  sh:description "The resulting claim."@en;
  sh:name "awards"@en;
  sh:severity sh:Violation;
  #sh:order 5;
.
edc:AwardingProcess-elm_awardingBody
  a sh:PropertyShape;  
  sh:path elm:awardingBody;
  sh:class elm:Agent;
  sh:minCount 1;
  sh:description "The awarding body related to this awarding activity (i.e., the organisation that issues the qualification). Only in cases of co-awarding/co-graduation, where a qualification is issued to an individual by two or more organisations, the cardinality is greater than 1."@en;
  sh:name "awarding body"@en;
  sh:severity sh:Violation;
  #sh:order 6;
.

edc:AwardingProcess-elm_educationalSystemNote
  a sh:PropertyShape;  
  sh:path elm:educationalSystemNote;
  sh:class elm:Note;
  sh:maxCount 1;
  sh:maxCount 1;
  sh:description "Additional information about the national system of this awarding process."@en;
  sh:name "educational system note"@en;
  sh:severity sh:Violation;
  #sh:order 7;
.

