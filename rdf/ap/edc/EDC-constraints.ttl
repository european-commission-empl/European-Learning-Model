 #---------------------------------------------------------------
 #--------  Verifiable Credential Shape ------------------------
 #---------------------------------------------------------------
 edc:VerifiableCredentialShape
  a sh:NodeShape ;
  sh:targetClass cred:VerifiableCredential;
  rdfs:comment "A Verifiable Credential. This is the superclass of European Digital credential for Learning."@en; 
  rdfs:label "Verifiable Credential "@en;
  sh:property edc:VerifiableCredential-cred_issuer;
  sh:property edc:VerifiableCredential-cred_credentialSubject;
  sh:property edc:VerifiableCredential-cred_issued;
  sh:property edc:VerifiableCredential-cred_issuanceDate;
  sh:property edc:VerifiableCredential-cred_validFrom;
  sh:property edc:VerifiableCredential-cred_validUntil;
  sh:property edc:VerifiableCredential-cred_expirationDate;
  sh:property edc:VerifiableCredential-cred_proof;
  sh:property edc:VerifiableCredential-cred_evidence;
  sh:property edc:VerifiableCredential-cred_termsOfUse;
  sh:property edc:VerifiableCredential-cred_credentialSchema;
  sh:property edc:VerifiableCredential-cred_credentialStatus;
  sh:property edc:VerifiableCredential-cred_holder;
  .

edc:VerifiableCredential-cred_issuer
  a sh:PropertyShape; 
  sh:path cred:issuer;
  sh:class elm:Agent;
  sh:description "The organisation that issued the credential and sealed it with its digital e-seal."@en;
  sh:name "issuer"@en;
  sh:minCount 1;
  sh:maxCount 1;
  sh:severity sh:Violation;
  sh:node edc:IssuerNodeShape;
  #sh:order 0;
  . 

edc:VerifiableCredential-cred_credentialSubject
  a sh:PropertyShape; 
  sh:path cred:credentialSubject;
  sh:class elm:Agent;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The subject (agent, person etc) about which claims are made and who owns the credential."@en;
  sh:name "credential subject (owner)"@en;
  sh:severity sh:Violation;
  #sh:order 1;
  . 
edc:VerifiableCredential-cred_issuanceDate
  a sh:PropertyShape; 
  sh:path cred:issuanceDate;
  sh:datatype xsd:dateTime;
  sh:maxCount 1;
  sh:description "Defines the date and time, when the credential becomes valid."@en;
  sh:name "issuance date"@en;
  sh:severity sh:Violation;
  #sh:order 2;
  .
 edc:VerifiableCredential-cred_issued
  a sh:PropertyShape; 
  sh:path cred:issued;
  sh:datatype xsd:dateTime;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The date when the credential was issued."@en;
  sh:name "issued"@en;
  sh:severity sh:Violation;
  #sh:order 3;
  .

edc:VerifiableCredential-cred_validFrom
  a sh:PropertyShape; 
  sh:path cred:validFrom;
  sh:datatype xsd:dateTime;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The earliest date when the information associated with the credential subject property became valid."@en;
  sh:name "valid from"@en;
  sh:severity sh:Violation;
  #sh:order 4;
  .

edc:VerifiableCredential-cred_expirationDate
  a sh:PropertyShape; 
  sh:path cred:expirationDate;
  sh:datatype xsd:dateTime;
  sh:maxCount 1;
  sh:description "The date and time when the credential expires."@en;
  sh:name "expiration date"@en;
  sh:severity sh:Violation;
  #sh:order 5;
  . 

  edc:VerifiableCredential-cred_validUntil
  a sh:PropertyShape; 
  sh:path cred:validUntil;
  sh:datatype xsd:dateTime;
  sh:maxCount 1;
  sh:description "The date and time, when the credential expires (automatic invalidation)."@en;
  sh:name "valid until"@en;
  sh:severity sh:Violation;
  #sh:order 6;
  .
edc:VerifiableCredential-cred_proof
  a sh:PropertyShape; 
  sh:path cred:proof;
  sh:datatype xsd:anyURI;
  sh:description "The proof of the verifiable credential."@en;
  sh:name "proof"@en;
  sh:severity sh:Violation;
  #sh:order 7;
  .
edc:VerifiableCredential-cred_evidence
  a sh:PropertyShape; 
  sh:path cred:evidence;
  sh:datatype elm:Evidence;
  sh:minCount 1;
  sh:description "The evidence of the verifiable credential."@en;
  sh:name "evidence"@en;
  sh:severity sh:Violation;
  #sh:order 8;
  .
 edc:VerifiableCredential-cred_credentialStatus
  a sh:PropertyShape; 
  sh:path cred:credentialStatus;
  sh:datatype xsd:anyURI;
  sh:maxCount 1;
  sh:description "The status of the verifiable credential."@en;
  sh:name "credential status"@en;
  sh:severity sh:Violation;
  #sh:order 9;
  . 
 edc:VerifiableCredential-cred_credentialSchema
  a sh:PropertyShape; 
  sh:path cred:credentialSchema;
  sh:datatype xsd:anyURI;
  sh:minCount 1;
  sh:description "The schema the verifiable credential should be validated against."@en;
  sh:name "credential schema"@en;
  sh:severity sh:Violation;
  #sh:order 10;
  . 
 edc:VerifiableCredential-cred_termsOfUse
  a sh:PropertyShape; 
  sh:path cred:termsOfUse;
  sh:datatype xsd:anyURI;
  sh:description "The terms of use of verifiable credential."@en;
  sh:name "terms of use"@en;
  sh:severity sh:Violation;
  #sh:order 11;
  .   
 edc:VerifiableCredential-cred_holder
  a sh:PropertyShape; 
  sh:path cred:holder;
  sh:class elm:Agent;
  sh:maxCount 1;
  sh:description "The holder of verifiable credential."@en;
  sh:name "holder"@en;
  sh:severity sh:Violation;
  #sh:order 12;
  .    
#---------------------------------------------------------------  
#--------------------- Claim Node Shape -----------------------
#---------------------------------------------------------------  
edc:ClaimNodeShape
   a sh:NodeShape ;
   rdfs:label "Claim Node " ;
   rdfs:comment "This shape is used for validation purposes, in case the range of a property is a Claim (superclass). A Claim Node is either a Learning Achievement Shape, a Learning Activity Shape, a Learning Assessment Shape, a Learning Entitlement Shape, and rarely it can be a Claim Type Node Shape, that is a Claim where the extra restriction of type property being mandatory is imposed. It is strongly recommended to use the subclasses of the Claim (e.g Learning Achievement, Learning Activity, Learning Assessment, Learning Entitlement) and only use the Claim superclass in the rare occasion when the claim provided is not covered by the existing subclasses. In such case, the node shape used for validation of the Claim is the Claim Type Node Shape, where it is mandatory to provide type of the claim using a controlled list."@en;
   sh:message "The node is a Claim, which is either a Learning Achievement, or a  Learning Activity, or a Learning Assessment or a Learning Entitlement and rarely a Claim with type property being mandatory." ;
   sh:or ([
           sh:class elm:LearningAchievement
         ] 
         [
           sh:class elm:LearningActivity
         ]
         [
           sh:class elm:LearningAssessment
         ]
         [
           sh:class elm:LearningEntitlement
         ] 
         [
           sh:node edc:ClaimTypeNodeShape
         ]
       ).


#---------------------------------------------------------------  
#-------------- Specification Node Shape -----------------------
#---------------------------------------------------------------  
edc:SpecificationNodeShape
   a sh:NodeShape ;
   rdfs:comment "A Specification Node is either a Learning Achievement Specification Shape, or a Learning Activity Specification Shape, or a Learning Assessment Specification Shape or a Learning Entitlement Specification  Shape. This shape is used for validation purposes, in case the range of a property is a Specification (superclass)." ;
   rdfs:label "Specification Node " ;
   sh:message "The node is a Specification, which is either a Learning Achievement Specification , or a  Learning Activity Specification , or a Learning Assessment Specification  or a Learning Entitlement Specification." ;
   sh:or ([
           sh:class elm:LearningAchievementSpecification
         ] 
         [
           sh:class elm:LearningActivitySpecification
         ]
         [
           sh:class elm:LearningAssessmentSpecification
         ]
         [
           sh:class elm:LearningEntitlementSpecification
         ] 
       ).

#-------------------------------------------------------------------
#----------------- Claim Type Node Shape ---------------------------
#------------------------------------------------------------------
edc:ClaimTypeNodeShape 
  a sh:NodeShape;
  rdfs:comment "This shape does not apply on a class. This shape checks if a node is a Claim (when used directly and not via one of its subclasses),  having type as a mandatory property."@en; 
  rdfs:label "Claim Type Node "@en; 
  sh:property[
  sh:path dc:title;
  sh:datatype rdf:langString;
  sh:minCount 1;
  sh:description "The title of the claim. One value per language is permitted."@en;
  sh:name "title"@en;
  sh:uniqueLang true ;
  sh:severity sh:Violation;
  #sh:order 0;
  ];
  sh:property[
  sh:path dc:description;
  sh:datatype rdf:langString;
  sh:description "A free text description of the claim."@en;
  sh:name "description"@en;
  sh:uniqueLang true ;
  sh:severity sh:Violation;
  #sh:order 1;
  ]; 
  sh:property[
  sh:path adms:identifier;
  sh:class elm:Identifier;
  sh:description "An identifier of the claim."@en;
  sh:name "identifier"@en;
  sh:severity sh:Violation;
  #sh:order 2;
  ];
  sh:property[
  sh:path elm:additionalNote;
  sh:class elm:Note;
  sh:maxCount 1;
  sh:description "An additional free text note about the claim."@en;
  sh:name "more information"@en;
  sh:severity sh:Violation;
  #sh:order 4;
  ];
  sh:property[  
  sh:path elm:supplementaryDocument;
  sh:class elm:WebResource;
  sh:description "A public web document containing additional documentation about the claim."@en;
  sh:name "other documents"@en;
  sh:severity sh:Violation;
  #sh:order 5;
  ];
  sh:property[
  sh:path elm:awardedBy;
  sh:class elm:AwardingProcess;
  sh:maxCount 1;
  sh:description "The awarding details of the claim."@en;
  sh:name "awarded by"@en;
  sh:severity sh:Violation;
  ];
  sh:property [
     sh:description "When the claim (superclass is used) the type is mandatory."@en;
     sh:name "type";
     sh:path dc:type;
     sh:class skos:Concept;
     sh:minCount 1;
     sh:severity sh:Violation;
  ]
  .
