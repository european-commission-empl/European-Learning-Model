#------ Claim class and properties shapes --------

edc:ClaimShape 
  a sh:NodeShape; 
  sh:targetClass elm:Claim; 
  rdfs:comment "A claim made by an issuer. This class carries properties common to all claims, including Learning Achievement, Learning Activity, Learning Assessment and Learning Entitlement. It serves as an extension point that enables the definition of additional subclasses."@en; 
  rdfs:label "Claim "@en; 
  sh:property edc:Claim-dc_title;
  sh:property edc:Claim-dc_description;
  sh:property edc:Claim-adms_identifier;
  sh:property edc:Claim-dc_type;
  sh:property edc:Claim-elm_additionalNote;
  sh:property edc:Claim-elm_supplementaryDocument;
  sh:property edc:Claim-elm_awardedBy;
  .
edc:Claim-dc_title
  a sh:PropertyShape; 
  sh:path dc:title;
  sh:datatype rdf:langString;
  sh:minCount 1;
  sh:description "The title of the claim. One value per language is permitted."@en;
  sh:name "title"@en;
  sh:uniqueLang true ;
  sh:severity sh:Violation;
  #sh:order 0;
  .
edc:Claim-dc_description
  a sh:PropertyShape; 
  sh:path dc:description;
  sh:datatype rdf:langString;
  sh:description "A free text description of the claim."@en;
  sh:name "description"@en;
  sh:uniqueLang true ;
  sh:severity sh:Violation;
  #sh:order 1;
  .
edc:Claim-adms_identifier
  a sh:PropertyShape; 
  sh:path adms:identifier;
  sh:class elm:Identifier;
  sh:description "An identifier of the claim."@en;
  sh:name "identifier"@en;
  sh:severity sh:Violation;
  #sh:order 2;
  .
edc:Claim-dc_type
  a sh:PropertyShape; 
  sh:path dc:type;
  sh:class skos:Concept;
  sh:description "The type of the claim. It should be provided using a controlled vocabulary. The users can also use their own controlled list(s) to specify subtypes of the Learning Achievement, Learning Activity, Learning Assessment and Learning Entitlement. Multiple types are allowed, but they should all fall under the same upper type (Learning Achievement, Learning Activity, Learning Assessment and Learning Entitlement), since the subclasses of Claim superclass are disjoint."@en;
  sh:name "type"@en;
  sh:severity sh:Violation;
  #sh:order 3;
  .
edc:Claim-elm_additionalNote
  a sh:PropertyShape; 
  sh:path elm:additionalNote;
  sh:class elm:Note;
  sh:maxCount 1;
  sh:description "An additional free text note about the claim."@en;
  sh:name "more information"@en;
  sh:severity sh:Violation;
  #sh:order 4;
  .  
  edc:Claim-elm_supplementaryDocument
  a sh:PropertyShape; 
  sh:path elm:supplementaryDocument;
  sh:class elm:WebResource;
  sh:description "A public web document containing additional documentation about the claim."@en;
  sh:name "other documents"@en;
  sh:severity sh:Violation;
  #sh:order 5;
  .
  edc:Claim-elm_awardedBy
  a sh:PropertyShape;
  sh:path elm:awardedBy;
  sh:class elm:AwardingProcess;
  sh:minCount 1;
  sh:maxCount 1;
  sh:description "The awarding details of the claim."@en;
  sh:name "awarded by"@en;
  sh:severity sh:Violation;
  #sh:order 6;
  .
