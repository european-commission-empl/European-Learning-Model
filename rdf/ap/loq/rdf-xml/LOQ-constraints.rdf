      <rdf:Description>
        <sh:class rdf:resource="http://data.europa.eu/snb/model/elm/LearningActivitySpecification"/>
      </rdf:Description>
      <rdf:Description>
        <sh:class rdf:resource="http://data.europa.eu/snb/model/elm/LearningAssessmentSpecification"/>
