  <sh:NodeShape rdf:about="http://data.europa.eu/snb/model/ap/ams-constraints/AddressShape">
    <sh:property rdf:parseType="Resource">
      <sh:severity rdf:resource="http://www.w3.org/ns/shacl#Violation"/>
      <sh:name xml:lang="en">country</sh:name>
      <sh:description xml:lang="en">The addressâ€™ country code. It should be provided using the Country Named Authority List.</sh:description>
      <sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
      >1</sh:maxCount>
      <sh:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
      >1</sh:minCount>
      <sh:class rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <sh:path rdf:resource="http://data.europa.eu/snb/model/elm/countryCode"/>
    </sh:property>
    <sh:property rdf:parseType="Resource">
      <sh:severity rdf:resource="http://www.w3.org/ns/shacl#Violation"/>
      <sh:name xml:lang="en">full address</sh:name>
      <sh:description xml:lang="en">The complete address with or without formatting.</sh:description>
      <sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
      >1</sh:maxCount>
      <sh:class rdf:resource="http://data.europa.eu/snb/model/elm/Note"/>
      <sh:path rdf:resource="http://data.europa.eu/snb/model/elm/fullAddress"/>
    </sh:property>
    <sh:property rdf:parseType="Resource">
      <sh:severity rdf:resource="http://www.w3.org/ns/shacl#Violation"/>
      <sh:name xml:lang="en">identifier</sh:name>
      <sh:description xml:lang="en">An address identifier.</sh:description>
