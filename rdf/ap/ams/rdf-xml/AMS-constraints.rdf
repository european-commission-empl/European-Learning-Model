      <sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
      <sh:path rdf:resource="http://data.europa.eu/snb/model/elm/contentUrl"/>
    </sh:property>
    <sh:property rdf:parseType="Resource">
