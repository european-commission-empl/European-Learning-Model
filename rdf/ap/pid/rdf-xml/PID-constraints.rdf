  <sh:NodeShape rdf:about="http://data.europa.eu/snb/model/ap/pid-constraints/GeometryShape">
    <sh:property rdf:parseType="Resource">
      <sh:severity rdf:resource="http://www.w3.org/ns/shacl#Violation"/>
      <sh:name xml:lang="en">latitude</sh:name>
      <sh:description xml:lang="en">The geographic coordinate that specifies the north / south position of the Geomerty on the Earth's surface.</sh:description>
      <sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <sh:path rdf:resource="http://data.europa.eu/m8g/latitude"/>
    </sh:property>
    <sh:property rdf:parseType="Resource">
      <sh:severity rdf:resource="http://www.w3.org/ns/shacl#Violation"/>
      <sh:name xml:lang="en">longitude</sh:name>
      <sh:description xml:lang="en">The geographic coordinate that specifies the east / west position of the Geometry on the Earth's surface.</sh:description>
      <sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <sh:path rdf:resource="http://data.europa.eu/m8g/longitude"/>
    </sh:property>
    <rdfs:label xml:lang="en">Geometry Shape</rdfs:label>
    <rdfs:comment xml:lang="en">This class defines the geometry at the conceptual level, and it can be encoded by using different formats.</rdfs:comment>
    <sh:targetClass rdf:resource="http://www.w3.org/ns/locn#Geometry"/>
  </sh:NodeShape>
  <sh:NodeShape rdf:about="http://data.europa.eu/snb/model/ap/pid-constraints/ConceptScheme">
    <rdfs:label xml:lang="en">Concept  Scheme Shape</rdfs:label>
