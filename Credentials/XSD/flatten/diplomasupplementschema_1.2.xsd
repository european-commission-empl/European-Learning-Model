            <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="text" type="ContentType">
                <xs:annotation>
                    <xs:documentation>A formatted character string (i.e. a finite set of characters)
                        generally in the form of words of a language.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ContentType">
        <xs:annotation>
            <xs:documentation>A formatted character string (i.e. a finite set of characters) in the
                form of words of a language.
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="lang" type="LanguageCharCodeEnumType" use="required">
