    <!--NoteType-->

    <xs:complexType name="NoteType">
        <xs:annotation>
            <xs:documentation>A formatted and annotated text fragment.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element form="qualified" maxOccurs="unbounded" minOccurs="1" name="text" type="ContentType">
                <xs:annotation>
                    <xs:documentation>A formatted character string (i.e. a finite set of characters)
                        generally in the form of words of a language.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="subject">
                <xs:annotation>
                    <xs:documentation>A subject or topic that is described in the
                        text.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="uri" type="SubjectTypeCodeType" use="optional">
                                <xs:annotation>
                                    <xs:documentation>The URI identifier of a known code that
                                        indicates/specifies the topic or subject of a text or
                                        document.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Code-->

    <xs:complexType name="CodeType">
        <xs:complexContent>
            <xs:extension base="AbstractCodeType">
                <xs:attribute name="targetFrameworkUrl" type="DocumentURLType" use="required">
                    <xs:annotation>
                        <xs:documentation>The URL of the framework or system used to describe the assoicated
                            object..
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AbstractCodeType" abstract="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">A term from a controlled vocabulary.
                A code is a character string of letters, numbers, special characters (except escape
                sequences), and symbols. It represents a definitive value, a method,
                or a property description in an abbreviated or language-independent form that is
                part of a finite list of allowed values. The CodeType is used for
                all elements that are used to enable a common coded value representation. Typical
                example of code types are: Country Code and Language Code. The
                CodeType should be used in case of a finite list of allowed values. The
                IdentifierType should be used in case of an infinite set of objects.
                This CodeType is a generic data type which is also used to represent association
                objects.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element form="qualified" maxOccurs="1" minOccurs="1" name="targetName"
                        type="TextType">
                <xs:annotation>
                    <xs:documentation>A name or label of the associated object in the targeted
                        framework.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0"
                        name="targetDescription" type="TextType">
                <xs:annotation>
                    <xs:documentation>A description of the associated object in the targeted
                        framework.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0"
                        name="targetFrameworkName" type="TextType">
                <xs:annotation>
                    <xs:documentation>The name of the framework or system used to describe the
                        associated object.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="uri" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">A URI identifier of the code. For URI identifier of
                    an associated object, USE targetID attribute instead of this.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="targetNotation" type="xs:token" use="optional">
            <xs:annotation>
                <xs:documentation>A notation or code of the associated object in the targeted
                    framework.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

