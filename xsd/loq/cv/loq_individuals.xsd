            <xs:enumeration value="http://data.europa.eu/snb/status/to-be-reviewed">
                <xs:annotation>
                    <xs:documentation>To Be Reviewed</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="http://data.europa.eu/snb/status/not-published-anymore">
                <xs:annotation>
                    <xs:documentation>Not Published Anymore</xs:documentation>
