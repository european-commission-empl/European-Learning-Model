   <!-- ComplexType FOR CLASSES  -->
   <xs:complexType name="AccreditationType">
      <xs:annotation>
         <xs:documentation>The quality assurance or licensing of an organisation or a qualification. An accreditation instance can be used to specify information about: - the quality assurance and/or licensing of an organisation. - the quality assurance and/or licensing of an organisation with respect to a specific qualification.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="identifier">
            <xs:annotation>
               <xs:documentation>An alternative Identifier of the Accreditation, as assigned to it by the accrediting agent.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="type" type="AccreditationTypeRestrictionType">
            <xs:annotation>
               <xs:documentation>The type of the accreditation</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="1" ref="title">
            <xs:annotation>
               <xs:documentation> The title of the accreditation. One value per language permitted. </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="description">
            <xs:annotation>
               <xs:documentation>The type as mandatory property. One value per language permitted. </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="decision">
            <xs:annotation>
               <xs:documentation>The Quality Decision issued by the Quality Assuring Authority.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="report">
            <xs:annotation>
               <xs:documentation>A publicly accessible report of the quality assurance decision.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="1" name="organisation" type="IdReferenceType">
            <xs:annotation>
               <xs:documentation>The organisation whose activities are being accredited. It MUST refer to an existing 'organisation'-record in the 'agentReferences'-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="limitQualification">
            <xs:annotation>
               <xs:documentation>The qualification that was accredited. It MUST refer to an existing 'qualification'-record in the 'learningAchivementSpecificationReferences'-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="limitField">
            <xs:annotation>
               <xs:documentation>The field of education for which the accreditation is valid.It should be provided using  ISCED-F contreolled vocabulary.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="limitEQFLevel">
            <xs:annotation>
               <xs:documentation>The european qualification level for which the accreditation is valid.It should be provided using  EQF contreolled vocabulary.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="limitJurisdiction">
            <xs:annotation>
               <xs:documentation>The jurisdiction for which the accreditation is valid. It should be provided using the Country Named Authority List.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="1" ref="accreditingAgent">
            <xs:annotation>
               <xs:documentation>The Quality Assuring Authority. (i.e assurer). It MUST refer to an existing 'organisation'-record in the 'agentReferences'-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="issued">
            <xs:annotation>
               <xs:documentation>The date when the sth was formally approved/issued.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="reviewDate">
            <xs:annotation>
               <xs:documentation>The date when the accreditation has to be re-viewed.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="expiryDate">
            <xs:annotation>
               <xs:documentation>The date when the accreditation expires or was expired..</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" ref="valid">
            <xs:annotation>
               <xs:documentation>The date when the accreditation became valid.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="additionalNote">
            <xs:annotation>
               <xs:documentation>An additional free text note about the resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="homepage">
            <xs:annotation>
               <xs:documentation>The homepage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="landingPage">
            <xs:annotation>
               <xs:documentation>The landing page of the accreditation.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="supplementaryDocument">
            <xs:annotation>
               <xs:documentation>A public web document containing additional documentation about the resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element ref="status" minOccurs="0">
            <xs:annotation>
               <xs:documentation>The publication status of an organisation.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="modified">
            <xs:annotation>
               <xs:documentation>The date when the accreditation was last updated since it was published.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element ref="publisher" minOccurs="0">
            <xs:annotation>
               <xs:documentation>The publisher of the Accreditation.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI" use="required"/>
   </xs:complexType>

