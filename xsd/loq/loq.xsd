   <xs:complexType name="LearningAchievementSpecificationType">
      <xs:annotation>
         <xs:documentation>A description of what a person may learn using the opportunity, expressed as learning outcomes. A specification of learning.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="RestrictedSpecificationType">
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="type" type="skos:ConceptType">
                  <xs:annotation>
                     <xs:documentation> The type of the Learning Opportunity.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element ref="learningOutcomeSummary" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>The full learning outcome description of the learning specification.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="1" ref="ISCEDFCode">
                  <xs:annotation>
                     <xs:documentation>Thematic Area according to the ISCED-F 2013 Classification.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="educationSubject">
                  <xs:annotation>
                     <xs:documentation>An associated field of education from another semantic framework than the ISCED classification.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="0" ref="volumeOfLearning">
                  <xs:annotation>
                     <xs:documentation>The estimated number of hours the learner is expected to spend engaged in learning to earn the award. This would include the notional number of hours in class, in group work, in practicals, as well as hours engaged in self-motivated study.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="creditPoint">
                  <xs:annotation>
                     <xs:documentation>points assigned to the learning specification, following an alternative educational credit system.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="educationLevel">
                  <xs:annotation>
                     <xs:documentation>An associated level of education within a semantic framework describing education levels.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" ref="language">
                  <xs:annotation>
                     <xs:documentation>The language.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="mode">
                  <xs:annotation>
                     <xs:documentation>The mode of learning, and or assessment</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="0" ref="learningSetting">
                  <xs:annotation>
                     <xs:documentation>The type of learning setting (formal, non-formal).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="0" ref="maximumDuration">
                  <xs:annotation>
                     <xs:documentation>The maximum duration of a learning opportunity for which this specification/qualification is designed. The maximum duration a learning opportunity that implements this specification/qualification should take.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="targetGroup">
                  <xs:annotation>
                     <xs:documentation>A specific target group or category for which this specification is designed.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="0" ref="entryRequirement">
                  <xs:annotation>
                     <xs:documentation>Specific entry requirements or prerequisites of individuals for which this specification is designed to start this learning opportunity.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="1" name="learningOutcome" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>An individual (expected) learning outcome of the learning specification. It MUST refer to an existing 'LearningOutcome'-record in the 'learningOutcomeReferences'-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="influencedBy">
                  <xs:annotation>
                     <xs:documentation>Activities which a person can perform to acquire the expected learning outcomes. It MUST refer to an existing 'LearningActivitySpecification'-record in the 'learningActivitySpecificationReferences'-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="provenBy">
                  <xs:annotation>
                     <xs:documentation>Assessments a person can undergo to prove the acquisition of the learning outcomes. It MUST refer to an existing 'LearningAssessmentSpecification'-record in the 'learningAssessmentSpecificationReferences'-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="entitlesTo">
                  <xs:annotation>
                     <xs:documentation>Rights (such as which the person may acquire as a result of acquiring the learning outcomes). It MUST refer to an existing 'LearningEntitlementSpecification'-record in the 'learningEntitlementSpecificationReferences'-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="hasPart" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A learning specification can be composed of other partial learning specifications which when combined make up this learning specification. It MUST refer to an existing 'learningAchievementSpecification'record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="specialisationOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A learning specification (e.g. a standard) of which this specification is a specialisation. It MUST refer to an existing 'learningAchievementSpecification'record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="isPartOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A learning achievement specification, which this learning achievement specification is part of. It MUST refer to an existing 'learningAchievementSpecification'record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="generalisationOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A learning specification (e.g. a standard) of which this specification is a generalisation. It MUST refer to an existing 'learningAchievementSpecification'record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="awardingOpportunity">
                  <xs:annotation>
                     <xs:documentation>Refers to an activity related to the awarding of the learning specification, such as the country or region where the qualification is awarded, the awarding body and optionally the awarding period now or in the past.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>

   </xs:complexType>

   <xs:complexType name="LearningActivitySpecificationType">
      <xs:annotation>
         <xs:documentation>The specification of a process which leads to the acquisition of knowledge, skills or responsibility and autonomy.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="RestrictedSpecificationType">
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="type"
                           type="LearningActivityTypesRestrictionType">
                  <xs:annotation>
                     <xs:documentation>The type of the learning activity. A concept indicating the type of the learning activity.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="contactHour">
                  <xs:annotation>
                     <xs:documentation>The contact hours.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="0" ref="volumeOfLearning">
                  <xs:annotation>
                     <xs:documentation>The estimated number of hours the learner is expected to spend engaged in learning to earn the award. This would include the notional number of hours in class, in group work, in practicals, as well as hours engaged in self-motivated study.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="language">
                  <xs:annotation>
                     <xs:documentation>The language of instruction.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="mode">
                  <xs:annotation>
                     <xs:documentation>The mode of learning and or assessment (i.e. online, blended, presential, work based) provided by a controlled vocabullary</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="influences">
                  <xs:annotation>
                     <xs:documentation>What may be learned by undertaking the activity described by this learning activity specification.
                        The expected learning outcomes to which this learning activity specification contributes to.
                        This is given by a Learning Achievement Specification.
                        A learning activity can lead to or contribute to the acquisition of a set of skills, knowdledge, autonomy and or responsibilty.
                        It MUST refer to an existing record in the 'learningAchievementSpecificationReferences-section.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="hasPart" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A learning activity specification can be composed of smaller learning specifications, which when combined make up this learning specification. It MUST refer to an existing 'learningActivitySpecification'record in the 'learningActivitySpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="specialisationOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>An activity specification (e.g. a standard) of which this specification is a specialisation. It MUST refer to an existing 'learningActivitySpecification'record in the 'learningActivitySpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="isPartOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A learning activity specification, which this learning activity specification is part of. It MUST refer to an existing 'learningActivitySpecification'record in the 'learningActivitySpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="generalisationOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>An activity specification (e.g. a standard) of which this specification is a generalisation. It MUST refer to an existing 'learningActivitySpecification'record in the 'learningActivitySpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="LearningAssessmentSpecificationType">
      <xs:annotation>
         <xs:documentation>A Learning Assessment Specification is a specification of a process establishing the extent to which a learner has attained particular knowledge, skills and competences against criteria such as learning outcomes or standards of competence.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="RestrictedSpecificationType">
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="type"
                           type="LearningAssessmentTypesRestrictionType">
                  <xs:annotation>
                     <xs:documentation>The type of the learning assessment. A concept indicating the type of the learning assessment.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="language">
                  <xs:annotation>
                     <xs:documentation>The language of instriction.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="mode">
                  <xs:annotation>
                     <xs:documentation>The mode of learning and or assessment (i.e. online, blended, presential, work based) provided by a controlled vocabullary</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="0" name="gradingScheme" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A description of the specification of which learning outcomes are or have been proven. It MUST refer to an existing 'gradingScheme" record in the 'gradingSchemeReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="proves">
                  <xs:annotation>
                     <xs:documentation>The learning achievement (and related learning outcomes) this assessment is designed to test. It MUST refer to an existing record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="hasPart" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>Smaller assessments specifications, which when combined make up this assessmentspecification. It MUST refer to an existing 'learningAssessmentSpecification'record in the 'learningAssessmentSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="specialisationOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>An assessment specification (e.g. a standard) of which this specification is a specialisation. It MUST refer to an existing 'learningAssessmentSpecification'record in the 'learningAssessmentSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="isPartOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A learning assessment specification, which this learning assessment specification is part of. It MUST refer to an existing 'learningAssessmentSpecification'record in the 'learningAssessmentSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="generalisationOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>An assessment specification (e.g. a standard) of which this specification is a generalisation. It MUST refer to an existing 'learningAssessmentSpecification'record in the 'learningAssessmentSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="LearningEntitlementSpecificationType">
      <xs:annotation>
         <xs:documentation>The specification of a right a person has access to, typically as a result of a learning achievement. It may take the form of the right to be a member of an organisation, to follow a certain learning opportunity specification, or to follow a certain career.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="RestrictedSpecificationType">
            <xs:sequence>
               <xs:element maxOccurs="1" minOccurs="1" name="type"
                           type="LearningEntitlementTypesRestrictionType">
                  <xs:annotation>
                     <xs:documentation>The type of the learning entitlement. A concept indicating the type of the learning entitlement.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="1" ref="entitlementStatus">
                  <xs:annotation>
                     <xs:documentation>The status.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="limitOrganisation">
                  <xs:annotation>
                     <xs:documentation>The organisation which acknowledges the entitlement (i.e. the organisation offering the learning opportunity, membership or employment opportunity). It MUST refer to an existing 'organisation' record in the 'agentReferences'-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="limitJurisdiction">
                  <xs:annotation>
                     <xs:documentation>The jurisdiction for which the entitlement is valid (the region or country).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="limitOccupation">
                  <xs:annotation>
                     <xs:documentation>The ESCO Occupation or Occupational class which the individual may access through the entitlement.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="limitNationalOccupation">
                  <xs:annotation>
                     <xs:documentation>An Occupation or Occupational Category.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="entitledBy">
                  <xs:annotation>
                     <xs:documentation>The learning achivement specification that is entiled by this learning entitlement specification. It MUST refer to an existing record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="hasPart" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>Smaller entitlement specifications, which when combined make up this entitlement specification. It MUST refer to an existing 'learningEntitlementSpecification'record in the 'learningEntitlementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="specialisationOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>An entitlement specification (e.g. a standard) of which this specification is a specialisation. It MUST refer to an existing 'learningEntitlementSpecification'record in the 'learningEntitlementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="isPartOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>A learning  entitlement specification, which this learning entitlement specification is part of. It MUST refer to an existing 'learningEntitlementSpecification'record in the 'learningEntitlementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="generalisationOf" type="IdReferenceType">
                  <xs:annotation>
                     <xs:documentation>An entitlement specification (e.g. a standard) of which this specification is a generalisation. It MUST refer to an existing 'learningEntitlementSpecification'record in the 'learningEntitlementSpecificationReferences-section.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="LearningOpportunityType">
      <xs:annotation>
         <xs:documentation>An opportunity to realise a given set of learning outcomes via a learning activity and/or assessment.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="identifier">
            <xs:annotation>
               <xs:documentation>An alternative Identifier of the Learning Opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="1" ref="title">
            <xs:annotation>
               <xs:documentation> The title of Learning Opportunity. One value per language permitted.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="description">
            <xs:annotation>
               <xs:documentation> A free text description. One value per lanuage permitted.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="type" type="LearningOpportunityTypesRestrictionType" minOccurs="0"
                     maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>The type of the learning opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="additionalNote">
            <xs:annotation>
               <xs:documentation>An additional free text note about the Learning Opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" ref="homepage">
            <xs:annotation>
               <xs:documentation>The homepage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="supplementaryDocument">
            <xs:annotation>
               <xs:documentation>A public web document containing additional documentation about the Learning Opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="temporal">
            <xs:annotation>
               <xs:documentation>The associated period of time shape.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="duration">
            <xs:annotation>
               <xs:documentation>The nominal duration for which the learning opportunity will continue to run. Note, this may be after the end-date, since admissions may be closed but the learning opportunity may still be ongoing.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="mode">
            <xs:annotation>
               <xs:documentation>The mode of learning Opportunity</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element ref="grant" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>The Grant related to the Learning Opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="learningSchedule">
            <xs:annotation>
               <xs:documentation>The learning schedule.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="scheduleInformation">
            <xs:annotation>
               <xs:documentation>Detailed information about the timetable or schedule. This may include weekly schedules (e.g. 'Every Monday, 4pm', but may also include the overall schedule for the course, (e.g. October: lectures, November: group-work, December: break, January: assessment).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="admissionProcedure">
            <xs:annotation>
               <xs:documentation>Specific information on how to apply for the course.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="applicationDeadline">
            <xs:annotation>
               <xs:documentation>The deadline for submitting an application to the learning opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="priceDetail">
            <xs:annotation>
               <xs:documentation>Information about the pricing of the course, including fees and scholarships/subsidies available.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="1" ref="providedBy">
            <xs:annotation>
               <xs:documentation>The organisation providing or directing the learning opportunity. In the case of, e.g. joint qualifications, there may be several organisations directing the learning opportunity. It MUST refer to an existing 'organisation'-record in the 'agentReferences'-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="location" type="IdReferenceType">
            <xs:annotation>
               <xs:documentation>The location where the learning opportunity will take place. This may also include a virtual space. It MUST refer to an existing 'location'-record in the'locationReferences'-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="1" name="learningAchievementSpecification" type="IdReferenceType">
            <xs:annotation>
               <xs:documentation> The learning achivement specification, including the curricula, of this learning opportunity. It MUST refer to an existing record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" name="learningActivitySpecification" type="IdReferenceType">
            <xs:annotation>
               <xs:documentation> The learning activity specification of this learning opportunity. It MUST refer to an existing 'learningActivitySpecification' record in the 'learningActivitySpecificationReferences-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="hasPart" type="IdReferenceType">
            <xs:annotation>
               <xs:documentation>A learning opportunity can be composed of other learning opportunities, which when combined make up this larger learning opportunity. It MUST refer to an existing 'learningOpportunity' record in the 'learningOpportunityReferences'-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" name="isPartOf" type="IdReferenceType">
            <xs:annotation>
               <xs:documentation>A learning opportunity, which this learning achievement specification is part of. It MUST refer to an existing 'learningOpportunity'record in the 'learningOpportunityReferences-section.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="bannerImage">
            <xs:annotation>
               <xs:documentation>An image which is displayed alongside the learning opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="1" ref="defaultLanguage">
            <xs:annotation>
               <xs:documentation>The base language of the learning opportunity, to be considered authoritative.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element ref="status" minOccurs="0">
            <xs:annotation>
               <xs:documentation>The publication status of the learning opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element minOccurs="0" ref="modified">
            <xs:annotation>
               <xs:documentation>The date when the learning opportunity was last modified.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element ref="publisher" minOccurs="0">
            <xs:annotation>
               <xs:documentation>The publisher of the learning opportunity.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI" use="required"/>
   </xs:complexType>

   <xs:complexType name="LearningOutcomeType">
      <xs:annotation>
         <xs:documentation>A statement regarding what a learner knows, understands and is able to do on completion of a learning process, which are defined in terms of knowledge, skills and responsibility and autonomy.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="identifier">
            <xs:annotation>
               <xs:documentation>An alternative Identifier of the Learning Outcome.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="1" ref="title">
            <xs:annotation>
               <xs:documentation>A legible, descriptive name for the learning outcome. One value per language permitted.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" name="type" type="LearningOutcomeTypesRestrictionType">
            <xs:annotation>
               <xs:documentation> The learning outcome type.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>An additional free text note about the Learning Outcome.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="1" minOccurs="0" ref="reusabilityLevel">
            <xs:annotation>
               <xs:documentation>The reusability level.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="relatedSkill">
            <xs:annotation>
               <xs:documentation>A link to a related skill or the level of a related skill on a skill framework (except ESCO).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element maxOccurs="unbounded" minOccurs="0" ref="relatedESCOSkill">
            <xs:annotation>
               <xs:documentation>A link to an ESCO  skill.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
      <xs:attribute name="id" type="xs:anyURI" use="required"/>
   </xs:complexType>

   <xs:complexType name="LegalIdentifierType">
      <xs:annotation>
         <xs:documentation>A Legal Identifier.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="IdentifierType">
            <xs:sequence>
               <xs:element maxOccurs="1" minOccurs="1" ref="spatial">
                  <xs:annotation>
                     <xs:documentation>A spatial identifier.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="LocationType">
      <xs:annotation>
         <xs:documentation>An identifiable geographic place.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="dc:LocationType">
