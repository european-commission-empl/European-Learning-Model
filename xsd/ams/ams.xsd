         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ContentTypeRestrictionType">
      <xs:complexContent>
         <xs:restriction base="skos:ConceptType">
            <xs:attribute name="uri" type="for:FileTypeEnumType" use="required"/>
         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ContentEncodingRestrictionType">
      <xs:complexContent>
         <xs:restriction base="skos:ConceptType">
            <xs:attribute name="uri" type="ecet:ContentEncodingTypesEnumType" use="required"/>
         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
   <xs:simpleType name="AllCountriesType">
      <xs:union memberTypes="cou:CountryEnumType nuts:NutsCodeEnumType"/>
   </xs:simpleType>
   <xs:complexType name="MultipleCountryRestrictionType">
      <xs:attribute name="uri" type="AllCountriesType" use="required"/>
   </xs:complexType>
   <xs:complexType name="AccreditationTypeRestrictionType">
      <xs:complexContent>
         <xs:restriction base="skos:ConceptType">
            <xs:attribute name="uri" type="eat:AccreditationTypesEnumType" use="required"/>
         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="CountryRestrictionType">
      <xs:complexContent>
         <xs:restriction base="skos:ConceptType">
            <xs:attribute name="uri" type="cou:CountryEnumType" use="required"/>
         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="AccreditedThematicAreaRestrictionType">
      <xs:complexContent>
         <xs:restriction base="skos:ConceptType">
            <xs:attribute name="uri" type="isced:ISCED-FEnumType" use="required"/>
         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="AccreditedEQFLevelRestrictionType">
      <xs:complexContent>
         <xs:restriction base="skos:ConceptType">
            <xs:attribute name="uri" type="eqf:EQFLevelEnumType" use="required"/>
         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="JurisdictionRestrictionType">
      <xs:complexContent>
         <xs:restriction base="skos:ConceptType">
            <xs:attribute name="uri" type="atu:AdministrativeTerritorialUnitEnumType" use="required"/>
         </xs:restriction>
      </xs:complexContent>
   </xs:complexType>
</xs:schema>
